/***** End of imports. If edited, may not auto-convert in the playground. *****/
var image= ee.ImageCollection(l8).filterBounds(geometry)
.filterDate('2016-07-20','2016-10-06')
.filterMetadata('CLOUD_COVER_LAND','less_than',10)
.sort('CLOUD_COVER_LAND')
.first()

print(image)

//set visualization parameters
var vis = {min:0, max:3000, gamma:1.4,bands:['B4','B3','B2']};

Map.addLayer(image,vis,'true colour image')

//set class name (make sure pick the object by point tool and convert it to feature collection)
var classname= urban.merge(water).merge(bareSoil).merge(field).merge(tree)
print(classname)

var bands = ['B2', 'B3', 'B4', 'B5', 'B6', 'B7'];

//train the image by using previous classes
var training = image.select(bands).sampleRegions({
  collection: classname,
  properties: ['landcover'],
  scale: 30
});
print(training);

//classify the image
var classifier = ee.Classifier.smileRandomForest(100).train({
  features: training,
  classProperty: 'landcover',
  inputProperties: bands
});

var classified = image.select(bands).classify(classifier);
print(classified)

//set visualization parameters for classification results
var viz = { min:0, max:4, palette: ['red','orange','blue','yellow','green']}

Map.addLayer(classified, viz,'classification svm');
Map.centerObject(geometry,10)

